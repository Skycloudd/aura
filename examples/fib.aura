fn main() {
    print(fib(1));
    print(fib(2));
    print(fib(3));
    print(fib(4));
    print(fib(5));
    print(fib(6));
    print(fib(7));
    print(fib(8));
    print(fib(9));
    print(fib(10));
}

fn fib(num) {
    if num < 2
        return num;
    else
        return fib(num - 1) + fib(num - 2);
}
